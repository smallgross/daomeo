<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- resultMap最终还是要将结果映射到pojo上，type就是指定映射到哪一个pojo -->
<mapper namespace="com.dao.daomeo.dao.AddMapper">
	<resultMap type="com.dao.daomeo.entity.Add"
		id="BaseResultMap">
		<result column="name" jdbcType="VARCHAR" property="name" />
		<result column="brain" jdbcType="VARCHAR" property="brain" />
		<result column="age" jdbcType="VARCHAR" property="age" />
	</resultMap>
	<insert id="insert" parameterType="com.dao.daomeo.entity.Add">
		insert into
		add(name,brain,age)values(#{name},#{brain},#{age})
	</insert>
	<select id="selectAll" parameterType="String"
		resultMap="BaseResultMap">
		select name,brain from add

	</select>
</mapper>